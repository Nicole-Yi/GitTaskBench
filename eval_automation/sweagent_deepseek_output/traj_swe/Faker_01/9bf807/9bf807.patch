+import sys
+sys.path.append("/data/data/agent_test_codebase/GitTaskBench/code_base/Faker")
+from faker import Faker
+import csv
+
+# Initialize Faker
+def generate_fake_users(output_file, num_records=100):
+    fake = Faker()
+    
+    # Generate fake data
+    users = []
+    for _ in range(num_records):
+        username = fake.user_name()
+        email = fake.email()
+        users.append({"Username": username, "Email": email})
+    
+    # Write to CSV
+    with open(output_file, mode='w', newline='') as file:
+        writer = csv.DictWriter(file, fieldnames=["Username", "Email"])
+        writer.writeheader()
+        writer.writerows(users)
+    
+    print(f"Successfully generated {num_records} fake user records in {output_file}")
+
+if __name__ == "__main__":
+    output_path = "/data/data/agent_test_codebase/GitTaskBench/eval_automation/output/Faker_01/users.csv"
+    generate_fake_users(output_path)
