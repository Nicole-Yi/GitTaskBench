+#!/usr/bin/env python3
+
+import os
+import subprocess
+from pathlib import Path
+
+# Input and output paths
+input_dir = "/data/data/agent_test_codebase/GitTaskBench/queries/Eparse_01/input/Eparse_01_input"
+output_dir = "/data/data/agent_test_codebase/GitTaskBench/eval_automation/output/Eparse_01"
+output_file = os.path.join(output_dir, "output.txt")
+
+# Ensure output directory exists
+os.makedirs(output_dir, exist_ok=True)
+
+# Initialize the output file
+with open(output_file, 'w') as f:
+    f.write("Excel Files and Sheet Information:\n")
+
+# Scan the input directory for Excel files
+for root, _, files in os.walk(input_dir):
+    for file in files:
+        if file.endswith(('.xlsx', '.xls')):
+            file_path = os.path.join(root, file)
+            # Run the Eparse CLI to get sheet information
+            result = subprocess.run(
+                ["python", "-m", "eparse.cli", "--file", file_path, "--verbose"],
+                capture_output=True, text=True
+            )
+            # Write the output to the file
+            with open(output_file, 'a') as f:
+                f.write(f"File: {file_path}\n")
+                f.write(f"Sheet Info:\n{result.stdout}\n\n")
